{
  "name": "chelsea-tracker-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Backend server for Chelsea Tracker",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "json-server": "json-server --watch db.json --port 3001",
